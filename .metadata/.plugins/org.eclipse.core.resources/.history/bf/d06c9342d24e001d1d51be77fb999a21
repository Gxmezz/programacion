public class perdida_evaluacion_continua {
    public static void main (String[] args) {
        Scanner teclado=new Scanner(System.in);
        int hF, hT, pc, ID; //Horas faltadas, horas totales y porcentaje.
        hT=256; //modulo_programacion
        
        System.out.println("Ingresa número de horas faltadas");
            hF=teclado.nextInt(); //usuario escribe su numero de faltas
        System.out.println("Ingresa ID");
            ID=teclado.nextInt();
        
    if (hF>hT*0.15)
        System.out.print("ID ha perdido la Evaluación Continua");
    else if(hF>hT*0.10)
        System.out.print("HaS superado el 10% de horas faltadas, con el 15% perdera el derecho a Evaluación Continua");
    else if(hF>hT*0.05)
        System.out.print("Has superado el 5% de horas faltadas, con el 15% perdera el derecho a Evaluación Continua");
    else if(hF<hT*0.05)
        System.out.print("Has faltado menos del 5% de las horas. ¡Sigue asi!");
    
 }
}